{
    "name": "VolumeUp",
    "description": "This is a simple chrome extension that allows you to control the volume of your computer with the keyboard.",
    "manifest_version": 3,
    "version": "1.0",
    "author": "Andres Caicedo",
    "icons": {
        "16": "icons/logo16.png",
        "64": "icons/logo64.png",
        "128": "icons/logo128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "action": {
        "default_icon": "icons/logo16.png",
        "default_title": "VolumeUp",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ]
}