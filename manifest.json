{
    "name": "VolumeUp",
    "description": "This is a simple chrome extension that allows you to control the volume of your computer with the keyboard.",
    "manifest_version": 1.0,
    "version": "1.0",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "action": {
        "default_icon": "icon16.png",
        "default_title": "VolumeUp",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ]
}